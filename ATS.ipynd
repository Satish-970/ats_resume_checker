        "import os\n",
        "import re\n",
        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "\n",
        "def fetch_jd_from_url(url):\n",
        "    \"\"\"Fetch and filter job description text from a given URL.\"\"\"\n",
        "    try:\n",
        "        response = requests.get(url, timeout=10)\n",
        "        response.raise_for_status()\n",
        "        soup = BeautifulSoup(response.text, \"html.parser\")\n",
        "\n",
        "        # Remove scripts, styles, navigation, and hidden elements\n",
        "        for element in soup([\"script\", \"style\", \"noscript\", \"header\", \"footer\", \"nav\", \"aside\"]):\n",
        "            element.decompose()\n",
        "\n",
        "        # Combine all text\n",
        "        all_text = '\\n'.join(soup.stripped_strings)\n",
        "        lower_text = all_text.lower()\n",
        "\n",
        "        # Keywords to detect relevant sections\n",
        "        section_titles = [\n",
        "            \"responsibilities\", \"requirements\", \"skills\", \"qualifications\",\n",
        "            \"what you will do\", \"what you'll do\", \"role\", \"desired skills\"\n",
        "        ]\n",
        "\n",
        "        jd_sections = []\n",
        "        for title in section_titles:\n",
        "            match = re.search(rf\"{title}(.+?)(\\n\\n|\\Z)\", lower_text, re.DOTALL)\n",
        "            if match:\n",
        "                section_text = match.group(1)\n",
        "                jd_sections.append(section_text)\n",
        "\n",
        "        # If no sections found, fallback to whole text\n",
        "        if not jd_sections:\n",
        "            jd_sections.append(all_text)\n",
        "\n",
        "        # Keep only bullet points or meaningful lines\n",
        "        filtered_lines = []\n",
        "        for sec in jd_sections:\n",
        "            for line in sec.split(\"\\n\"):\n",
        "                line = line.strip()\n",
        "                if line.startswith((\"-\", \"•\", \"*\")) or len(line.split()) > 2:\n",
        "                    filtered_lines.append(line)\n",
        "\n",
        "        final_jd = \"\\n\".join(filtered_lines)\n",
        "        return final_jd.strip()\n",
        "\n",
        "    except Exception as e:\n",
        "        print(f\"Error fetching job description from URL: {e}\")\n",
        "        return \"\"\n",
        "\n",
        "def run_ats_checker():\n",
        "    print(\"ATS Resume Checker\")\n",
        "    print(\"=\" * 50)\n",
        "    print(\"This tool analyzes how well your resume matches a job description.\")\n",
